{
  "name": "soft",
  "version": "0.1.0",
  "private": true,
  "scripts": {
    "dev": "next dev",
    "build": "next build",
    "start": "next start",
    "lint": "next lint",
    "check-types": "tsc --pretty --noEmit",
    "check-format": "prettier --check ..",
    "check-lint": "eslint .. --ext ts --ext tsx --ext js",
    "format": "prettier --write ..",
    "fix": "eslint .. --ext ts --ext tsx --ext js --fix",
    "test-all": "npm run check-format && npm run check-lint && npm run check-types && npm run build"
  },
  "dependencies": {
    "@reduxjs/toolkit": "^1.8.0",
    "axios": "^0.26.0",
    "next": "12.1.0",
    "react": "17.0.2",
    "react-dom": "17.0.2",
    "react-redux": "^7.2.6",
    "redux-logger": "^3.0.6",
    "redux-persist": "^6.0.0"
  },
  "devDependencies": {
    "@types/node": "17.0.21",
    "@types/react": "17.0.39",
    "@types/redux-logger": "^3.0.9",
    "eslint": "8.10.0",
    "eslint-config-next": "12.1.0",
    "nodemon": "^2.0.15",
    "typescript": "4.6.2"
  }
}
